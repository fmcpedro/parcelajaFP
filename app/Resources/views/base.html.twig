    <!-- Apply the plugin -->
    <script>
            var table = $('#tableWithExportOptions');

            var settings = {
                "sDom": "<'exportOptions'T><'table-responsive't><'row'<p i>>",
                "sPaginationType": "bootstrap",
                "destroy": true,
                "scrollCollapse": true,
                "oLanguage": {
                    "sLengthMenu": "_MENU_ ",
