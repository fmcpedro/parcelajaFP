{% extends 'base.html.twig' %}

{% block body %}
    <h1>Binbank</h1>

    <table>
        <tbody>
            <tr>
                <th>Countryname</th>
                <td>{{ binBank.countryName }}</td>
            </tr>
            <tr>
                <th>Countrycode</th>
                <td>{{ binBank.countryCode }}</td>
            </tr>
            <tr>
                <th>Brand</th>
                <td>{{ binBank.brand }}</td>
            </tr>
            <tr>
                <th>Bank</th>
                <td>{{ binBank.bank }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ binBank.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_binbank_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_binbank_edit', { 'id': binBank.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
