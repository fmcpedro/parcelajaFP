{# empty Twig template #}








<!-- BEGIN SIDEBAR -->
<div class="page-sidebar" data-pages="sidebar">
    <div id="appMenu" class="sidebar-overlay-slide from-top">
    </div>
    <!-- BEGIN SIDEBAR HEADER -->
    <div class="sidebar-header">
        <img src="assets/img/logo_white.png" alt="logo" class="brand" data-src="assets/img/logo_white.png" data-src-retina="assets/img/logo_white_2x.png" width="78" height="22">
        <div class="sidebar-header-controls">
            <button data-pages-toggle="#appMenu" class="btn btn-xs sidebar-slide-toggle btn-link m-l-20" type="button"><i class="fa fa-angle-down fs-16"></i>
            </button>
            <button data-toggle-pin="sidebar" class="btn btn-link visible-lg-inline" type="button"><i class="fa fs-12"></i>
            </button>
        </div>
    </div>
    <!-- END SIDEBAR HEADER -->
    <!-- BEGIN SIDEBAR MENU -->
    <div class="sidebar-menu">
        <ul class="menu-items">

            
            {% set route = app.request.attributes.get('_route')%}
            
            <li class="{% if route in ['admin_payments_simulator','admin_payments_index', 'admin_payments_next_payments','admin_payments_payment_forecasts' ] %}active{% endif %}">
                <a href="javascript:;">
                    <span class="title">Transações</span>
                    <span class=" arrow"></span>
                </a>
                <span class="icon-thumbnail"><i class="pg-phone"></i></span>
                <ul class="sub-menu">

                    {% if is_granted('ROLE_BNI') %} 
                        <li class="{% if route in ['admin_payments_simulator'] %}active{% endif %}">
                            <a href="{{ path('admin_payments_simulator') }}">Simulador</a>
                            <span class="icon-thumbnail">SI</span>
                        </li>
                    {% endif %}                    

                    {% if is_granted('ROLE_BNI') %} 
                        <li class="{% if route in ['admin_payments_index'] %}active{% endif %}">
                            <a href="{{ path('admin_payments_index') }}">Capturadas </a>
                            <span class="icon-thumbnail">TP</span>
                        </li>
                    {% endif %}

                    {% if is_granted('ROLE_BNI') %} 
                        <li class="{% if route in ['admin_payments_next_payments'] %}active{% endif %}">
                            <a href="{{ path('admin_payments_next_payments') }}">Por capturar </a>
                            <span class="icon-thumbnail">TP</span>
                        </li>
                    {% endif %}
                    
                    {% if is_granted('ROLE_ADMIN') %} 
                        <li class="{% if route in ['admin_payments_payment_forecasts'] %}active{% endif %}">
                            <a href="{{ path('admin_payments_payment_forecasts') }}">Agregadas </a>
                            <span class="icon-thumbnail">TP</span>
                        </li>
                    {% endif %}
                    
                    
                    

                </ul>
            </li>


            {% if is_granted('ROLE_ADMIN') %} 

                <li class="{% if route in ['admin_agency_index','admin_broker_index' ] %}active{% endif %}">
                    <a href="javascript:;">
                        <span class="title">Lojas</span>
                        <span class=" arrow"></span>
                    </a>
                    <span class="icon-thumbnail"><i class="pg-phone"></i></span>
                    <ul class="sub-menu">
                        <li class="{% if route in ['admin_agency_index' ] %}active{% endif %}">
                            <a href="{{ path('admin_agency_index') }}">Lojas</a>
                            <span class="icon-thumbnail">TI</span>
                        </li>
                        <li class="{% if route in ['admin_broker_index' ] %}active{% endif %}">
                            <a href="{{ path('admin_broker_index') }}">Brokers</a>
                            <span class="icon-thumbnail">TP</span>
                        </li>

                    </ul>
                </li>
            {% endif %}
            
            
            


            {% if is_granted('ROLE_ADMIN') %} 

                <li class="{% if route in ['admin_tpos_index','admin_terminal_payment_index' ] %}active{% endif %}">
                    <a href="javascript:;">
                        <span class="title">Terminais</span>
                        <span class=" arrow"></span>
                    </a>
                    <span class="icon-thumbnail"><i class="pg-phone"></i></span>
                    <ul class="sub-menu">
                        <li class="{% if route in ['admin_tpos_index'] %}active{% endif %}">
                            <a href="{{ path('admin_tpos_index') }}">Instalados</a>
                            <span class="icon-thumbnail">TI</span>
                        </li>
                        <li class="{% if route in ['admin_terminal_payment_index' ] %}active{% endif %}">
                            <a href="{{ path('admin_terminal_payment_index') }}">Pagamentos </a>
                            <span class="icon-thumbnail">TP</span>
                        </li>

                    </ul>
                </li>
            {% endif %}
            
            
            {% if is_granted('ROLE_ADMIN') %} 

                <li class="{% if route in ['admin_binbank_index', 'admin_bincode_index'] %}active{% endif %}">
                    <a href="javascript:;">
                        <span class="title">Bins</span>
                        <span class=" arrow"></span>
                    </a>
                    <span class="icon-thumbnail"><i class="pg-phone"></i></span>
                    <ul class="sub-menu">
                        <li class="{% if route in ['admin_binbank_index'] %}active{% endif %}">
                            <a href="{{ path('admin_binbank_index') }}">Bancos</a>
                            <span class="icon-thumbnail">TI</span>
                        </li>
                        <li class="{% if route in ['admin_bincode_index'] %}active{% endif %}">
                            <a href="{{ path('admin_bincode_index') }}">Bins </a>
                            <span class="icon-thumbnail">TP</span>
                        </li>

                    </ul>
                </li>
            {% endif %}
            
            
            
            
            {% if is_granted('ROLE_ADMIN') %} 

                <li class="{% if route in ['admin_post_index', 'admin_media_index'] %}active{% endif %}">
                    <a href="javascript:;">
                        <span class="title">Website</span>
                        <span class=" arrow"></span>
                    </a>
                    <span class="icon-thumbnail"><i class="pg-phone"></i></span>
                    <ul class="sub-menu">
                        <li class="{% if route in ['admin_post_index'] %}active{% endif %}">
                            <a href="{{ path('admin_post_index') }}">Posts</a>
                            <span class="icon-thumbnail">TI</span>
                        </li>
                        <li class="{% if route in ['admin_media_index'] %}active{% endif %}">
                            <a href="{{ path('admin_media_index') }}">Noticias</a>
                            <span class="icon-thumbnail">TP</span>
                        </li>

                    </ul>
                </li>
            {% endif %}
            
            
            
            
        </ul>
        <div class="clearfix"></div>
    </div>
    <!-- END SIDEBAR MENU -->
</div>
<!-- END SIDEBAR -->