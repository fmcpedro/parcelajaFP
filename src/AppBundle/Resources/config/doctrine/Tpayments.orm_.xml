<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Tpayments" repository-class="AppBundle\Repository\TpaymentsRepository" table="TPayments">
    <indexes>
      <index name="FRefContractNum" columns="FRefContractNum"/>
    </indexes>
    <id name="fpurchaseid" type="integer" column="FPurchaseID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="frefcontractnum" type="string" column="FRefContractNum" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fpayid" type="string" column="FPayID" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="finstallment" type="smallint" column="FInstallment" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="famount" type="float" column="FAmount" precision="10" scale="0" nullable="false"/>
    <field name="fdate" type="date" column="FDate" nullable="false"/>
    <field name="famountevo" type="float" column="FAmountEVO" precision="10" scale="0" nullable="true"/>
    <field name="fdateevo" type="date" column="FDateEVO" nullable="true"/>
    <field name="fchargebackevo" type="float" column="FChargebackEVO" precision="10" scale="0" nullable="true"/>
  </entity>
</doctrine-mapping>
