<?php

namespace AppBundle\Entity;

/**
 * Tcategory
 */
class Tcategory
{
    /**
     * @var string
     */
    private $fcatname;

    /**
     * @var string
     */
    private $fdescript;

    /**
     * @var integer
     */
    private $fcategoryid;


    /**
     * Set fcatname
     *
     * @param string $fcatname
     *
     * @return Tcategory
     */
    public function setFcatname($fcatname)
    {
        $this->fcatname = $fcatname;

        return $this;
    }

    /**
     * Get fcatname
     *
     * @return string
     */
    public function getFcatname()
    {
        return $this->fcatname;
    }

    /**
     * Set fdescript
     *
     * @param string $fdescript
     *
     * @return Tcategory
     */
    public function setFdescript($fdescript)
    {
        $this->fdescript = $fdescript;

        return $this;
    }

    /**
     * Get fdescript
     *
     * @return string
     */
    public function getFdescript()
    {
        return $this->fdescript;
    }

    /**
     * Get fcategoryid
     *
     * @return integer
     */
    public function getFcategoryid()
    {
        return $this->fcategoryid;
    }
}
